{
  "name": "gitpractise-starwars-name",
  "version": "0.0.0-development",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "cover": "nyc npm t",
    "watch:test": "npm t -- -w",
    "test": "mocha src/index.test.js -w --require babel-core/register",
    "semantic-release": "semantic-release",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "prebuild": "rimraf dist",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kripa644/gitPractise-starwars-name.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "author": "kripa644 <kripa@robosoftin.com> (http://kripa644.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kripa644/gitPractise-starwars-name/issues"
  },
  "homepage": "https://github.com/kripa644/gitPractise-starwars-name#readme",
  "dependencies": {
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "@babel/register": "7.13.8",
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.3.4",
    "codecov.io": "0.1.6",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "ghooks": "2.0.4",
    "mocha": "8.3.2",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "semantic-release": "^17.4.2"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
